{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "paths": {
      "@configs/*": ["./src/configs/*"],
      "@controllers/*": ["./src/controllers/*"],
      "@db/*": ["./src/db/*"],
      "@middleware/*": ["./src/middleware/*"],
      "@models/*": ["src/db/models/*"],
      "@routers/*": ["./src/routers/*"],
      "@routes/*": ["./src/routes/*"],
      "@appTypes/*": ["./src/types/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "typeRoots": ["src/types/CustomExpress.d.ts"]
  },
  "include": ["src"]
}
